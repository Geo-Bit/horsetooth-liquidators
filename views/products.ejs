<html>
<head>
  <title>Products - Horsetooth Liquidators Inc.</title>
</head>
<body>
  <h1>Our Products</h1>
  <ul>
    <% products.forEach(function(product) { %>
      <li><a href='/products/<%= product.id %>'><%= product.name %></a> - $<%= product.price %></li>
    <% }); %>
  </ul>
  <a href='/'>Back to Home</a>
</body>
</html>